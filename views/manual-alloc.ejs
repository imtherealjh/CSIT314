<%-include('partials/head.ejs')%>
  <link rel="stylesheet" href="./css/manual-alloc.css" />
  <script type="text/javascript" src="./js/manual-alloc.js"></script>
  </head>

  <body>
    <div class="container">
      <h1>Cyber security paper</h1>
      <div class="input-group rounded">
        <input id="searchList" type="search" class="form-control rounded" placeholder="Search" aria-label="Search"
          aria-describedby="search-addon" />
        <span class="input-group-text border-0" id="search-addon">
          <i class="fas fa-search"></i>
        </span>
      </div>
      <form id="allocate-reviewers" method="post">
        <div class="three-items-container">
          <div class="select-label-container">
            <label>Not allocated</label>
            <select class="form-select searchTableList" size="20" multiple>
              <option value="1">One</option>
              <option value="2">Two</option>
              <option value="3">Three</option>
            </select>
          </div>
          <div class="two-item-up-down-container">
            <% if((locals.alloc) ? alloc : false) { %>
              <i class="fa-solid fa-angles-right"></i>
            <% } %>
            <% if((locals.unalloc) ? unalloc : false) { %>
              <i class="fa-solid fa-angles-left"></i>
            <% } %>
          </div>
          <div class="select-label-container">
            <label>Allocated</label>
            <select name="temp-allocated" class="form-select searchTable" size="20" multiple>
              <option value="1">One</option>
              <option value="2">Two</option>
              <option value="3">Three</option>
            </select>
          </div>
        </div>
        <input type="hidden" name="selected">
        <button type="submit" class="btn btn-primary btn-lg btn-block font-weight-bold">SUBMIT</button>
      </form>
    </div>
    <script></script>
  </body>

  </html>