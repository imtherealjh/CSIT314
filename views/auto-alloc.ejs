<%-include('partials/head.ejs')%>
  </head>

  <body>
    <div class="container">
      <h1>Automatic Allocation Of Papers</h1>
      <form method="post">
        <div class="list-group">
          <% for(let [idx, obj] of data.entries() ?? []) { %>
            <label class="list-group-item">
              <input class="form-check-input me-1" type="checkbox" name="papers" value="<%=obj%>">
              <%=obj%>
            </label>
            <% } %>
        </div>
        <button type="submit" class="btn btn-primary btn-lg btn-block">Submit</button>
      </form>
    </div>

    <script></script>
  </body>

  </html>